<h1>Listing profs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profs.each do |prof| %>
  <tr>
    <td><%= prof.name %></td>
    <td><%= prof.email %></td>
    <td><%= link_to 'Show', prof %></td>
    <td><%= link_to 'Edit', edit_prof_path(prof) %></td>
    <td><%= link_to 'Destroy', prof, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Prof', new_prof_path %>
